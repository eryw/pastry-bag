{
    "name": "eryw/pastry-bag",
    "description": "A plugin for CakePHP 3 provides the constructor dependencies injection and the controller method injection",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "dependency", "injection", "ioc", "container"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ery Widyanto",
            "email": "kristeryw@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/eryw/pastry-bag/issues"
    },
    "require": {
        "cakephp/cakephp": ">=3.2.0 <4.0",
        "cakephp/plugin-installer": "*",
        "aura/di": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "PastryBag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PastryBag\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp",
            "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src",
            "Company\\TestPluginThree\\": "tests/test_app/Plugin/Company/TestPluginThree/src"
        }
    },
    "minimum-stability": "dev"
}
